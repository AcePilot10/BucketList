<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BucketList.Mobile.Views.SearchView">
  <ContentView.Content>
      <StackLayout>
            <StackLayout Orientation="Horizontal">
                <Entry x:Name="txtSearchQuery" Placeholder="Enter Search Query Here"/>
                <Button Text="Search"/>
            </StackLayout>
      </StackLayout>
  </ContentView.Content>
</ContentView>
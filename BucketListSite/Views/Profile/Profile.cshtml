@using BucketList.Entities.Models
@using Microsoft.AspNetCore.Identity

@inject UserManager<User> UserManager
@model User

@{
    ViewData["Title"] = "Profile";
}

@if (Model != null && Model.UserName.Equals(User.Identity.Name))
{
    <partial name="MyProfile.cshtml" />
}
else
{
    <partial name="PublicProfile.cshtml" />
}